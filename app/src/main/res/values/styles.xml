<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="default_font">
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textColor">@color/black_b01</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="default_font.regular">
        <item name="android:fontFamily">@font/notosans_regular</item>
    </style>

    <style name="default_font.regular.26">
        <item name="lineHeight">26sp</item>
    </style>

    <style name="default_font.regular.22">
        <item name="lineHeight">22sp</item>
    </style>

    <style name="default_font.regular.20">
        <item name="lineHeight">20sp</item>
    </style>

    <style name="default_font.regular.18">
        <item name="lineHeight">18sp</item>
    </style>

    <style name="default_font.regular.15">
        <item name="lineHeight">15sp</item>
    </style>

    <style name="default_font.medium">
        <item name="android:fontFamily">@font/notosans_medium</item>
    </style>

    <style name="default_font.medium.42">
        <item name="lineHeight">42sp</item>
    </style>

    <style name="default_font.medium.36">
        <item name="lineHeight">36sp</item>
    </style>

    <style name="default_font.medium.27">
        <item name="lineHeight">27sp</item>
    </style>

    <style name="default_font.medium.24">
        <item name="lineHeight">24sp</item>
    </style>

    <style name="default_font.medium.23">
        <item name="lineHeight">23sp</item>
    </style>

    <style name="default_font.medium.21">
        <item name="lineHeight">21sp</item>
    </style>

    <style name="default_font.bold">
        <item name="android:fontFamily">@font/notosans_bold</item>
    </style>

    <style name="default_font.bold.26">
        <item name="lineHeight">26sp</item>
    </style>

    <style name="default_font.bold.15">
        <item name="lineHeight">15sp</item>
    </style>

    <style name="header1" parent="default_font.medium.42">
        <item name="android:textSize">28sp</item>
    </style>

    <style name="header2" parent="default_font.medium.36">
        <item name="android:textSize">24sp</item>
    </style>

    <style name="header3" parent="default_font.medium.27">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="header4" parent="default_font.bold.26">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="header5" parent="default_font.medium.24">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="header6" parent="default_font.medium.21">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="body1" parent="default_font.regular.26">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="body2" parent="default_font.medium.23">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="body3" parent="default_font.regular.22">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="body4" parent="default_font.regular.20">
        <item name="android:textSize">13sp</item>
    </style>

    <style name="body5" parent="default_font.bold.15">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="body6" parent="default_font.regular.15">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="label1" parent="default_font.regular.26">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="label2" parent="default_font.regular.20">
        <item name="android:textSize">13sp</item>
    </style>

    <style name="label3" parent="default_font.regular.18">
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="not_defined_12_reg" parent="default_font.regular">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="not_defined_11_reg" parent="default_font.regular">
        <item name="android:textSize">11sp</item>
    </style>

    <style name="not_defined_11_reg.16" parent="default_font.regular">
        <item name="lineHeight">16sp</item>
    </style>

    <style name="not_defined_10_reg" parent="default_font.regular">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="finpo_main_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:background">@drawable/selector_main_button_bg</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:textColor">@color/selector_main_button_text</item>
        <item name="android:fontFamily">@font/notosans_medium</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="finpo_main_button.disable" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:fontFamily">@font/notosans_medium</item>
        <item name="android:background">@drawable/bg_solid_g08_rounded_5</item>
        <item name="android:textColor">@color/gray_g02</item>
    </style>

    <style name="finpo_close_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:background">@drawable/bg_solid_none_rounded_g06_5</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:textColor">@color/gray_g02</item>
        <item name="android:fontFamily">@font/notosans_regular</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="finpo_horizontal_line">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/gray_g05</item>
    </style>


    <style name="BottomNavigationText" parent="@android:style/TextAppearance">
        <item name="fontFamily">@font/notosans_regular</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="BottomNavigationText.Selected">
        <item name="fontFamily">@font/notosans_bold</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="Widget.App.Spinner" parent="@style/Widget.AppCompat.Spinner">
        <item name="overlapAnchor">true</item>
        <item name="android:background">@drawable/selector_spinner_down</item>
    </style>

    <style name="TabTextStyle">
        <item name="fontFamily">@font/notosans_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TabTextSelected" parent="header4">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/point_p01</item>
    </style>

    <style name="TabTextUnSelected" parent="body1">
        <item name="android:textColor">@color/black_b01</item>
    </style>

</resources>